{
  "name": "flask-vgs",
  "publisher": "thorcc",
  "repository": {
    "type": "git",
    "url": "https://github.com/thorcc/flask-vgs.git"
  },
  "displayName": "flask-vgs",
  "description": "",
  "version": "0.1.4",
  "engines": {
    "vscode": "^1.0.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:flask-vgs.createFlask",
    "onCommand:flask-vgs.runFlask"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "flask-vgs.createFlask",
        "title": "Create Flask"
      },
      {
        "command": "flask-vgs.runFlask",
        "title": "Run Flask"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "flask-vgs.createFlask",
          "group": "FlaskGroup@1"
        },
        {
          "command": "flask-vgs.runFlask",
          "group": "FlaskGroup@1"
        }
      ]
    }
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.4",
    "@types/mocha": "^9.0.0",
    "@types/node": "14.x",
    "@types/vscode": "^1.0.0",
    "@vscode/test-electron": "^1.6.2",
    "eslint": "^8.1.0",
    "glob": "^7.1.7",
    "mocha": "^9.1.3",
    "typescript": "^4.4.4"
  }
}
